package selfProgramming;

public class WhileLoops2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 0;
		
		for (int i = 0; i < 10; i++) {
			System.out.println("i is less than 10: " +i);
		
		}
		
		while (a < 10) {
			System.out.println("a is less than 10: " + a);
			a++;
		}

	}

}
